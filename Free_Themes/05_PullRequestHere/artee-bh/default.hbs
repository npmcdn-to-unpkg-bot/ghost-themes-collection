<!doctype html>
<!--[if IE 9]><html class="no-js lt-ie10" lang="en"><![endif]-->
<!--[if gt IE 9]><!--><html class="no-js" lang="en"><!--<![endif]-->
	<head>
		<meta charset="utf-8">
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
		<title>{{meta_title}}</title>
		<meta name="description" content="{{meta_description}}" />
		<link rel="shortcut icon" href="{{asset "img/favicon.png"}}" type="image/png">
		<link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">
		<link href='http://fonts.googleapis.com/css?family=Playfair+Display:400,700,900&amp;subset=latin,latin-ext' rel='stylesheet' type='text/css'>
		<link href='https://fonts.googleapis.com/css?family=Noto+Serif:400,700' rel='stylesheet' type='text/css'>
		<link rel="stylesheet" href="{{asset "css/vendor/normalize.css"}}">
		<link rel="stylesheet" href="{{asset "css/vendor/foundation.min.css"}}">
		<link rel="stylesheet" href="{{asset "css/vendor/mono-social-fonts.css"}}">
		<link rel="stylesheet" href="{{asset "css/vendor/owl.carousel.css"}}">
		<link rel="stylesheet" href="{{asset "css/vendor/owl.theme.css"}}">
		<link rel="stylesheet" href="{{asset "css/vendor/magnific-popup.css"}}">
		<link rel="stylesheet" href="{{asset "css/vendor/twentytwenty.css"}}">
		<link rel="stylesheet" href="{{asset "css/vendor/highlight.github.css"}}">
		<link rel="stylesheet" href="{{asset "css/style.css"}}">
		<link rel="stylesheet" href="{{asset "css/post.css"}}">
		<script src="{{asset "js/vendor/modernizr.js"}}"></script>
		{{ghost_head}}
	</head>
	{{!-- body background image defined in index.hbs --}}
	<body class="{{body_class}}">

	{{! sticky header }}
	{{> "sticky-header"}}

	{{! search module - using GhostHunter js plugin}}
	{{> "search"}}

	{{! Everything else gets inserted here }}
	{{{body}}}

	{{! footer section }}
	<footer class="artee-site-footer">
		<div class="row">
			 <div class="artee-social-icons small-12 columns">
				{{! you can add or remove social icons here, remember to add your URLs to a href="..."
					you can find other icons here : http://drinchev.github.io/monosocialiconsfont/
					it is better to use it with unicodes. IE don't support plain text.
				}}
				<a href="#" title="Behance"><span class="symbol">&#xe409;</span></a>
				<a href="#" title="DeviantArt"><span class="symbol">&#xe418;</span></a>
				<a href="#" title="Dribble"><span class="symbol">&#xe421;</span></a>
				<a href="#" title="Email"><span class="symbol">&#xe424;</span></a>
				<a href="#" title="Facebook"><span class="symbol">&#xe427;</span></a>
				<a href="#" title="Flickr"><span class="symbol">&#xe429;</span></a>
				<a href="#" title="LinkedIn"><span class="symbol">&#xe452;</span></a>
				<a href="#" title="MySpace"><span class="symbol">&#xe459;</span></a>
				<a href="#" title="Pinterest"><span class="symbol">&#xe464;</span></a>
				<a href="#" title="Picassa"><span class="symbol">&#xe463;</span></a>
				<a href="{{@blog.url}}/rss/" title="RSS" class="icon-feed"><span class="symbol">&#xe471;</span></a>
				<a href="#" title="Twitter"><span class="symbol">&#xe486;</span></a>
				<a href="#" title="Youtube"><span class="symbol">&#xe499;</span></a>
				<a href="#" title="Instagram"><span class="symbol">&#xe500;</span></a>
			 </div>
			 <div class="small-12 columns">
				 <section class="copyright">All content copyright <a href="/">{{@blog.title}}</a> &copy; 2014 &bull; All rights reserved.</section>
				 <section class="poweredby">Proudly published with <a class="icon-ghost" href="http://ghost.org">Ghost</a></section>
			 </div>
		</div>
	</footer>

	{{! footer section end }}
	<div class="artee-back-to-top hidden {{#is 'post'}}post-go-top{{/is}}">
		<i class="fa fa-angle-up"></i>
	</div>

	<div class="artee-body-bg-overlay"></div>

	{{!-- jQuery --}}
	<script src="{{asset "js/vendor/jquery-1.11.3.min.js"}}"></script>

	{{! Ghost outputs important scripts and data with this tag }}
	{{ghost_foot}}

	{{! js scripts section }}
	<script>
	    // needed for search (when blog is in subfolder)
	    var blogURL = '{{@blog.url}}'
	</script>
	<script src="{{asset "js/vendor/fastclick.js"}}"></script>
	<script src="{{asset "js/vendor/foundation.min.js"}}"></script>
	<script src="{{asset "js/vendor/owl.carousel.min.js"}}"></script>
	<script src="{{asset "js/vendor/magnific-popup.js"}}"></script>
	<script src="http://maps.google.com/maps/api/js?sensor=true"></script>
	<script src="{{asset "js/vendor/gmaps.js"}}"></script>
	<script src="{{asset "js/vendor/gmaps-styles.js"}}"></script>
	<script src="{{asset "js/vendor/jquery.shuffle.min.js"}}"></script>
	<script src="{{asset "js/vendor/imagesloaded.pkgd.min.js"}}"></script>
	<script src="{{asset "js/vendor/jquery.event.move.js"}}"></script>
	<script src="{{asset "js/vendor/jquery.twentytwenty.js"}}"></script>
	<script src="{{asset "js/vendor/headroom.js"}}"></script>
	<script src="{{asset "js/vendor/jquery.headroom.js"}}"></script>
	<script src="{{asset "js/vendor/ghost-hunter.js"}}"></script>
	<script src="{{asset "js/vendor/reading-time.js"}}"></script>
	<script src="{{asset "js/vendor/highlight.min.js"}}"></script>
	<script src="{{asset "js/artee.js"}}"></script>
	{{! js scripts section end }}

</body>
</html>